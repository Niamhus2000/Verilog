module sixbit_xor
(
  input wire [5:0] a, b,
  output wire [5:0] x
);

//6 bitwise xors
xorbit bit0 (.a(a[0]), .b(b[0]), .x(x[0]));
xorbit bit1 (.a(a[1]), .b(b[1]), .x(x[1]));
xorbit bit2 (.a(a[2]), .b(b[2]), .x(x[2]));
xorbit bit3 (.a(a[3]), .b(b[3]), .x(x[3]));
xorbit bit4 (.a(a[4]), .b(b[4]), .x(x[4]));
xorbit bit5 (.a(a[5]), .b(b[5]), .x(x[5]));
endmodule
